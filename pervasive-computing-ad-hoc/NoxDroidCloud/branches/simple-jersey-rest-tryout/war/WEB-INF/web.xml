<!--   service
  http://localhost:8888/rest3/todos/
  http://localhost:8888/rest3/todos/1
 -->  
 <display-name>dk.itu.noxdroid</display-name>
  <servlet>
    <servlet-name>Jersey REST Service 3</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>com.sun.jersey.config.property.packages</param-name>
      <param-value>dk.itu.noxdroid.resources</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Jersey REST Service 3</servlet-name>
    <url-pattern>/rest3/*</url-pattern>
  </servlet-mapping>
	
