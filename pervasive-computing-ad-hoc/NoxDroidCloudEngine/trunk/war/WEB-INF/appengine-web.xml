<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>noxdroidcloudengine</application>
  <version>1</version>

  <!--
    By default, App Engine sends requests serially to a given web server.
    To allow App Engine to send multiple requests in parallel specify:

      <threadsafe>true</threadsafe>
  -->
  
  <threadsafe>true</threadsafe>
  
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="noxdroidcloudengine.noxdelta" value="0.5"/>
    <property name="noxdroid.green_uppperbound" value="0.4"/>
    <property name="noxdroid.yellow_uppperbound" value="0.6"/>
  </system-properties>
 
</appengine-web-app>
